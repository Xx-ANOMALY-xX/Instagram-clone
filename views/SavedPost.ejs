<% include ./partials/header.ejs %>
  <div class="w-full min-h-screen bg-zinc-900 text-white py-5">
    <div class="nav flex gap-3 items-center px-4 ">
      <a href="/profile" class="text-2xl "><i class="ri-arrow-left-line"></i></a>
      <h3 class="text-xl font-medium -mt-1">Save Posts</h3>
    </div>
    <div class="posts mt-6 mb-20 flex flex-wrap gap-1">
      <% posts.reverse().forEach(function(post){%>
        <a href="/openPost/<%=post._id%>">
          <div class="post w-[32.5vw] aspect-square ">
            <img src="/images/uploads/<%=post.postImg%>" class="h-full w-full object-cover" alt="">
          </div>
        </a>
        <%})%>
    </div>
  </div>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/gsap.min.js"
    integrity="sha512-7eHRwcbYkK4d9g/6tD/mhkf++eoTHwpNM9woBxtPUBWm67zeAfFC+HrdoE2GanKeocly/VxeLvIqwvCdk7qScg=="
    crossorigin="anonymous" referrerpolicy="no-referrer"></script>


  <% include ./partials/footer.ejs %>